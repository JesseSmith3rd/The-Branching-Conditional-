// LetsPratice.cpp : Defines the entry point for the console application.
//
#include "stdafx.h"
#include <string>
#include <iostream>
using namespace std;



int main(int argc, char ** argv)
{
	const int iOne = 1;
	const int iTwo = 2;
	const int iTwenty = 20;
	const int iThree = 3;
	const int iFour = 4;

	int x = 2;

	switch (x) {
	case iOne:
		puts("one");
		break;
	case iTwo:
	case iTwenty:
		puts("two or twenty");
		break;
	case iThree:
		puts("three");
		break;
	case iFour:
		puts("four");
		break;

	default:
		puts("default");
		break;
	}

	return 0;
}
